{
  "name": "vote",
  "description": "基于 Egg + Vue + Webpack3 单页面服务端客户端渲染同构工程骨架项目.",
  "version": "1.0.0",
  "scripts": {
    "debug": "egg-bin debug",
    "build": "cross-env easy clean && easy build prod",
    "build:dev": "cross-env easy clean && easy build dev",
    "build:test": "cross-env easy clean && easy build test",
    "build:prod": "cross-env easy clean && easy build prod",
    "start": "egg-bin dev",
    "start:test": "cross-env EGG_SERVER_ENV=test node index.js",
    "deploy:prod": "cross-env EGG_SERVER_ENV=prod pm2 start start.json",
    "deploy:docker": "cross-env EGG_SERVER_ENV=prod pm2-runtime start.json",
    "start:prod": "cross-env EGG_SERVER_ENV=prod NODE_ENV=production node index.js",
    "lint": "eslint .",
    "fix": "eslint --fix .",
    "docker": "docker run -p 8080:7001 raoul1996/vote:latest",
    "docker:prod": "docker run -p 8080:7001 -d raoul1996/vote:latest"
  },
